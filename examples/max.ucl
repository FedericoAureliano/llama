module main
{
  input a : bv32;
  input b : bv32;

  var c : bv32;

  @synthesis function max2 (x : bv32, y : bv32) : bv32;

  init {
    c = max2(a, b);
  }
  
  next {
    c' = max2(a, b);
  }
  
  theorem bigger_than_a : (c >= a);
  lemma bigger_than_b : (c >= b);
  
  control {
    induction(1);
  }
}
