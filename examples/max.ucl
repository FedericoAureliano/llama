module main
{
  input a : integer;
  input b : integer;

  var c : integer;

  @synthesis function max2 (x : integer, y : integer) : integer;

  init {
    c = max2(a, b);
  }
  
  next {
    c' = max2(a, b);
  }
  
  theorem bigger_than_a : (c >= a);
  lemma bigger_than_b : (c >= b);
  
  control {
    induction;
  }
}
